---
import Layout from '../../layouts/Layout.astro';
import Preloader from '../../components/shared/preloader.astro';
import SidebarArea from "../../components/shared/sidebar-area.astro"
import Footer from '../../components/shared/footer.astro';
import HeaderTwo from '../../components/shared/headerTwo.astro';
---

<Layout title="Equipo">
    <!-- header-area-start -->
    <HeaderTwo></HeaderOne>
    <!-- /.Main Header -->

    <SidebarArea></SidebarArea>
    <!--/.sidebar-area-->

    <Preloader></Preloader>
    <!-- ./ preloader -->

    <div id="smooth-wrapper">
        <div id="smooth-content">

        <section class="page-header" data-background="/assets/img/bg-img/page-header-bg-equipo.jpg">
            <div class="overlay"></div>
            <div class="shapes">
                <div class="shape shape-1"><img src="/assets/img/shapes/page-header-shape-1.png" alt="shape"></div>
                <div class="shape shape-2"><img src="/assets/img/shapes/page-header-shape-2.png" alt="shape"></div>
                <div class="shape shape-3"><img src="/assets/img/shapes/page-header-shape-3.png" alt="shape"></div>
            </div>
            <div class="container">
                <div class="page-header-content text-center">
                    <h1 class="title">Miembros del equipo </h1>
                    <h4 class="sub-title"><a class="home" href="/">Inicio </a><div style="display: inline-block;" class="inner-page" >Nuestro equipo</div></h4>
                </div>
            </div>
        </section>
        <!-- ./ page-header -->

        <section class="team-section pt-130 pb-130">
            <div class="container">
                <div class="row gy-5 fade-wrapper">
                    <div class="col-lg-3 col-md-6">
                        <div class="team-item fade-top">
                            <div class="team-thumb">
                                <div class="gradient-color"></div>
                                <img src="/assets/img/team/team-5.png" alt="equipo">
                                <ul class="team-social-2">
                                    <li class="facebook">
                                        <a href="#"><i class="fab fa-facebook-f"></i></a>
                                    </li>
                                    <li class="pinterest">
                                        <a href="#"><i class="fab fa-pinterest"></i></a>
                                    </li>
                                    <li class="twitter">
                                        <a href="#"><i class="fab fa-twitter"></i></a>
                                    </li>
                                    <li class="instagram">
                                        <a href="#"><i class="fab fa-instagram"></i></a>
                                    </li>
                                </ul>
                            </div>
                            <div class="team-content">
                                <h3 class="title"><a >Humberto Aviño</a></h3>
                                <span>In-hacker, hacker de hackers</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="team-item fade-top">
                            <div class="team-thumb">
                                <div class="gradient-color"></div>
                                <img src="/assets/img/team/team-6.png" alt="equipo">
                                <ul class="team-social-2">
                                    <li class="facebook">
                                        <a href="#"><i class="fab fa-facebook-f"></i></a>
                                    </li>
                                    <li class="pinterest">
                                        <a href="#"><i class="fab fa-pinterest"></i></a>
                                    </li>
                                    <li class="twitter">
                                        <a href="#"><i class="fab fa-twitter"></i></a>
                                    </li>
                                    <li class="instagram">
                                        <a href="#"><i class="fab fa-instagram"></i></a>
                                    </li>
                                </ul>
                            </div>
                            <div class="team-content">
                                <h3 class="title"><a >Desire Viloria</a></h3>
                                <span>Asesora Legal Corporativa</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="team-item fade-top">
                            <div class="team-thumb">
                                <div class="gradient-color"></div>
                                <img src="/assets/img/team/team-7.png" alt="equipo">
                                <ul class="team-social-2">
                                    <li class="facebook">
                                        <a href="#"><i class="fab fa-facebook-f"></i></a>
                                    </li>
                                    <li class="pinterest">
                                        <a href="#"><i class="fab fa-pinterest"></i></a>
                                    </li>
                                    <li class="twitter">
                                        <a href="#"><i class="fab fa-twitter"></i></a>
                                    </li>
                                    <li class="instagram">
                                        <a href="#"><i class="fab fa-instagram"></i></a>
                                    </li>
                                </ul>
                            </div>
                            <div class="team-content">
                                <h3 class="title"><a >Edgidio León</a></h3>
                                <span>Ingeniero de Proyectos</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="team-item fade-top">
                            <div class="team-thumb">
                                <div class="gradient-color"></div>
                                <img src="/assets/img/team/team-8.png" alt="equipo">
                                <ul class="team-social-2">
                                    <li class="facebook">
                                        <a href="#"><i class="fab fa-facebook-f"></i></a>
                                    </li>
                                    <li class="pinterest">
                                        <a href="#"><i class="fab fa-pinterest"></i></a>
                                    </li>
                                    <li class="twitter">
                                        <a href="#"><i class="fab fa-twitter"></i></a>
                                    </li>
                                    <li class="instagram">
                                        <a href="#"><i class="fab fa-instagram"></i></a>
                                    </li>
                                </ul>
                            </div>
                            <div class="team-content">
                                <h3 class="title"><a >Carlos Ramírez</a></h3>
                                <span>Especialista en Innovación Tecnológica</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- ./ team-section -->

        <Footer></Footer>
        <!-- ./ footer-section -->

        </div>
    </div>
</Layout>

